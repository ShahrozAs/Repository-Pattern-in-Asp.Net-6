
@{
    ViewData["Title"] = "Contact";
    // Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Contact</h1>

@{

    if (TempData["data1"] != null)
    {

        foreach (var item in (int[])TempData["data1"])
        {
            <h3>@item</h3>
        }
    }

    else
    {
        <h1>No Data Found</h1>
    }
}